{
	"name": "reddit-view",
	"displayName": "Reddit View",
	"description": "Browse Reddit in VS Code",
	"publisher": "ekarbe",
	"version": "2.0.1",
	"icon": "static/RedditView_icon.png",
	"repository": "https://github.com/ekarbe/reddit-view",
	"galleryBanner.color": "#EE4848",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:redditview.start"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "redditview.start",
				"title": "Reddit"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"node-fetch": "^2.3.0"
	}
}
